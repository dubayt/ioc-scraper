{
    "manifest_version": 2,
    "name": "IOC Scraper",
    "version": "1.0",
    "description": "Extract IOCs from web pages",
    "permissions": ["activeTab", "storage"],
    "sidebar_action": {
        "default_panel": "popup.html",
        "default_title": "IOC Scraper",
        "browser_style": false
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"],
            "all_frames": true
        }
    ],
    "background": {
        "scripts": ["background.js"]
    }
}